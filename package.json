{
  "name": "tarea3",
  "private": true,
  "scripts": {
    "dev": "concurrently -n FRONT,API -c green,cyan \"npm:dev:front\" \"npm:dev:api\"",
    "dev:front": "npm --prefix frontend run dev",
    "dev:api": "npm --prefix backend run dev",

    "build": "npm --prefix frontend run build",
    "serve:prod": "npm run build && npm --prefix backend run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
